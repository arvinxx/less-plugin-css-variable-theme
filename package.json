    "babel-plugin-add-module-exports": "^1.0.4",
    "commitlint": "^11.0.0",
    "commitlint-config-gitmoji": "^2.2.1",
    "conventional-changelog-gitmoji-config": "^1.4.0",
    "diff": "^5.0.0",
    "eslint": "^7.19.0",
    "less": "~3.13.1",
    "father-build": "^1.19.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.7",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
